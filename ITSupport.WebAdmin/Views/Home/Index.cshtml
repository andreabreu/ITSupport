@{
    ViewBag.Title = "Home Page";
    ViewBag.User = "Andre Abreu";
}
<div class="container">
    <!-- start: PAGE HEADER -->
    <div class="row">
        <div class="col-sm-12">

            <!-- start: PAGE TITLE & BREADCRUMB -->
            <ol class="breadcrumb">
                <li>
                    <i class="clip-home-3"></i>
                    <a href="~/">
                        Home
                    </a>
                </li>
                <li class="active">
                    @ViewBag.Title
                </li>
                <li class="search-box">
                    <form class="sidebar-search">
                        <div class="form-group">
                            <input type="text" placeholder="Buscar...">
                            <button class="submit">
                                <i class="clip-search-3"></i>
                            </button>
                        </div>
                    </form>
                </li>
            </ol>
            <div class="page-header">
                <h1>
                    Dashboard <small>Visão geral &amp; Status </small>
                </h1>
            </div>
            <!-- end: PAGE TITLE & BREADCRUMB -->
        </div>
    </div>
    <!-- end: PAGE HEADER -->
    <!-- start: PAGE CONTENT -->
    <div class="row">
        <div class="col-sm-4">
            <div class="core-box">
                <div class="heading">
                    <i class="clip-user-4 circle-icon circle-green"></i>
                    <h2>Gerenciar Usuarios</h2>
                </div>
                <div class="content">
                    Liste todos usuarios do dominio e veja e modifique detalhes da conta, acessos, grupos ingressados, etc...
                </div>
                <a class="view-more" href="#">
                    Acessar <i class="clip-arrow-right-2"></i>
                </a>
            </div>
        </div>
        <div class="col-sm-4">
            <div class="core-box">
                <div class="heading">
                    <i class="clip-clip circle-icon circle-teal"></i>
                    <h2>Gerenciar Inventario</h2>
                </div>
                <div class="content">
                    Controle todo o inventario, equipamentos alocados em todas as filiais Aon Brasil.
                </div>
                <a class="view-more" href="#">
                    Acessar <i class="clip-arrow-right-2"></i>
                </a>
            </div>
        </div>
        <div class="col-sm-4">
            <div class="core-box">
                <div class="heading">
                    <i class="clip-mobile circle-icon circle-bricky"></i>
                    <h2>Telefonia</h2>
                </div>
                <div class="content">
                    Gerencie toda <strong>Telefonia Movel</strong> e <strong>Telefonia Fixa</strong> em estoque e em uso...
                </div>
                <a class="view-more" href="#">
                    Acessar <i class="clip-arrow-right-2"></i>
                </a>
            </div>
        </div>
    </div>
